<Window x:Class="AutoUpdate.VersionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoUpdate"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Title="生成版本" Height="187.5" Width="265">
    <Grid Background="#F0F0F0">
        <Label x:Name="label" Content="生成新版本成功!" FontSize="18" HorizontalAlignment="Left" Margin="59,34,0,0" VerticalAlignment="Top" Width="145"/>
        <Button x:Name="button" Content="确认" Click="CloseWindow" HorizontalAlignment="Left" Margin="92,93,0,0" VerticalAlignment="Top" Height="25" Width="75"/>

    </Grid>
</Window>
