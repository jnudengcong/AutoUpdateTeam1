<Window x:Class="AutoUpdate.VersionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoUpdate"
        mc:Ignorable="d"
        Title="生成版本" Height="187.5" Width="444">
    <Grid Background="#F0F0F0">
        <Label x:Name="label" Content="生成新版本成功!" FontSize="18" HorizontalAlignment="Left" Margin="35,21,0,0" VerticalAlignment="Top" Width="180"/>
        <ProgressBar Value="100" HorizontalAlignment="Left" Height="30" Margin="58,59,0,0" VerticalAlignment="Top" Width="310"/>
        <Button x:Name="button" Content="确认" Click="CloseWindow" HorizontalAlignment="Left" Margin="175,110,0,0" VerticalAlignment="Top" Height="25" Width="75"/>

    </Grid>
</Window>
